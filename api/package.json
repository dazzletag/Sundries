{
  "name": "sundries-api",
  "version": "0.1.0",
  "description": "Fastify + Prisma backend for Sundries SaaS",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --prefer-ts-exts src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed:admin": "ts-node-dev --transpile-only src/scripts/seedAdmin.ts"
  },
  "postinstall": "prisma generate",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@carehq/carehq-js": "^1.0.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/sensible": "^6.0.4",
    "@prisma/client": "^6.19.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.7.1",
    "fastify-plugin": "^5.1.0",
    "jose": "^2.0.6",
    "pdfkit": "^0.17.2",
    "pino": "^10.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@types/pdfkit": "^0.17.4",
    "prisma": "^6.19.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
